# Set your Azure subscription ID, resource group name, Data Factory name, pipeline name, and dataset name
$subscriptionId = "<YourSubscriptionId>"
$resourceGroupName = "<YourResourceGroupName>"
$dataFactoryName = "<YourDataFactoryName>"
$pipelineName = "<YourPipelineName>"
$datasetName = "<YourDatasetName>"

# Sign in to Azure account
az login

# Set the current subscription
az account set --subscription $subscriptionId

# Delete the pipeline
az datafactory pipeline delete --resource-group $resourceGroupName --factory-name $dataFactoryName --name $pipelineName

# Delete the dataset
az datafactory dataset delete --resource-group $resourceGroupName --factory-name $dataFactoryName --name $datasetName
